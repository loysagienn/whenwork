  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clear-lib": "rm -rf './lib'",
    "babel": "npm run clear-lib && NODE_ENV=production BABEL_ENV=production babel src --out-dir lib",
    "babel-dev": "npm run clear-lib && NODE_ENV=development BABEL_ENV=development babel src --out-dir lib -w",
    "webpack": "NODE_ENV=production npx webpack",
    "webpack-dev": "NODE_ENV=development npx webpack --watch --progress",
    "build": "npm run babel & npm run webpack",
    "build-dev": "npm run babel-dev & npm run webpack-dev",
    "start": "export NODE_ENV=production && node lib/server/index.js",
    "start-dev": "export NODE_ENV=development && nodemon lib/server/index.js",
    "eslint": "eslint src"
  },
